-- input a list and find its length

module getLength where

open import Data.Nat
open import Data.Bool

data List (A : Set) : Set where
  []   : List A
  _::_ : A -> List A -> List A

length : {x : Set} → List x → ℕ
length [] = 0
length (_ :: xs) = 1 + length xs

ex : List ℕ
ex = 1 :: (2 :: (3 :: []))

-- load with C-c C-l
-- type C-c C-n, and write length ex
